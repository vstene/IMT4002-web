<?xml version='1.0' encoding='utf-8'?>
<widget id="com.field.reporter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Field Reporter</name>
    <description>
        Field Report registration for IMT 4002
    </description>
    <author email="jvstene@gmail.com" href="https://github.com/vstene">
         Vegard Stene
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
